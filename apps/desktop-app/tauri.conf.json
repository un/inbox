{
  "productName": "UnInbox",
  "version": "0.0.1",
  "identifier": "com.uninbox",
  "build": {
    "beforeDevCommand": "cd ../web-app && pnpm dotenv -e ../../.env.local -- turbo run dev",
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "cd ../web-app && pnpm dotenv -e ../../.env.local -- turbo run build",
    "frontendDist": "../web-app/dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "UnInbox",
        "width": 1200,
        "height": 900
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
