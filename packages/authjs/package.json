{
  "name": "@auth/core",
  "version": "0.17.0",
  "description": "Authentication for the Web.",
  "keywords": [
    "authentication",
    "authjs",
    "jwt",
    "oauth",
    "oidc",
    "passwordless",
    "standard",
    "vanilla",
    "webapi"
  ],
  "homepage": "https://authjs.dev",
  "repository": "https://github.com/nextauthjs/next-auth.git",
  "author": "Bal치zs Orb치n <info@balazsorban.com>",
  "contributors": [
    "Bal치zs Orb치n <info@balazsorban.com>",
    "Nico Domino <yo@ndo.dev>",
    "Lluis Agusti <hi@llu.lu>",
    "Thang Huu Vu <hi@thvu.dev>",
    "Iain Collins <me@iaincollins.com"
  ],
  "type": "module",
  "types": "./index.d.ts",
  "files": [
    "*.js",
    "*.d.ts*",
    "lib",
    "providers",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./adapters": {
      "types": "./adapters.d.ts"
    },
    "./errors": {
      "types": "./errors.d.ts",
      "import": "./errors.js"
    },
    "./jwt": {
      "types": "./jwt.d.ts",
      "import": "./jwt.js"
    },
    "./providers": {
      "types": "./providers/index.d.ts"
    },
    "./providers/*": {
      "types": "./providers/*.d.ts",
      "import": "./providers/*.js"
    },
    "./types": {
      "types": "./types.d.ts"
    }
  },
  "license": "ISC",
  "dependencies": {
    "@panva/hkdf": "1.0.4",
    "@simplewebauthn/browser": "8.3.4",
    "@simplewebauthn/server": "8.3.6",
    "cookie": "0.6.0",
    "jose": "5.2.0",
    "oauth4webapi": "2.8.1",
    "preact": "10.19.3",
    "preact-render-to-string": "6.3.1"
  },
  "peerDependencies": {
    "nodemailer": "6.8.0"
  },
  "peerDependenciesMeta": {
    "nodemailer": {
      "optional": true
    }
  },
  "scripts": {
    "build": "pnpm css && pnpm providers && tsc",
    "clean": "rm -rf *.js *.d.ts* lib providers",
    "css": "node scripts/generate-css",
    "dev:auth": "pnpm css && pnpm providers && tsc -w",
    "providers": "node scripts/generate-providers"
  },
  "devDependencies": {
    "@types/cookie": "0.6.0",
    "@types/node": "20.11.13",
    "@types/nodemailer": "6.4.14",
    "@types/react": "18.2.46",
    "autoprefixer": "10.4.17",
    "postcss": "8.4.33",
    "postcss-nested": "6.0.1"
  }
}